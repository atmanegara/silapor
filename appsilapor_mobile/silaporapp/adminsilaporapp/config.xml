<?xml version="1.0" encoding="utf-8"?>
<widget id="io.hallotapinbastariadmin" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>Halo Tapin</name>
  <description>Aplikasi aduan dan layanan masyarakat   </description>
  <author email="dev@cordova.apache.org" href="http://cordova.io">TIM IT</author>
  <access origin="*" />
  <allow-intent href="http://*/*" />
  <allow-intent href="https://*/*" />
  <allow-intent href="tel:*" />
  <allow-intent href="sms:*" />
  <allow-intent href="mailto:*" />
  <allow-intent href="geo:*" />
  <platform name="android">
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icons/android/icon-36-ldpi.png" />
    <icon density="mdpi" src="res/icons/android/icon-48-mdpi.png" />
    <icon density="hdpi" src="res/icons/android/icon-72-hdpi.png" />
    <icon density="xhdpi" src="res/icons/android/icon-96-xhdpi.png" />
  </platform>
  <platform name="android">
    <splash density="land-hdpi" src="res/screens/android/screen-hdpi-landscape.png" />
    <splash density="land-ldpi" src="res/screens/android/screen-ldpi-landscape.png" />
    <splash density="land-mdpi" src="res/screens/android/screen-mdpi-landscape.png" />
    <splash density="land-xhdpi" src="res/screens/android/screen-xhdpi-landscape.png" />
    <splash density="port-hdpi" src="res/screens/android/screen-hdpi-portrait.png" />
    <splash density="port-ldpi" src="res/screens/android/screen-ldpi-portrait.png" />
    <splash density="port-mdpi" src="res/screens/android/screen-mdpi-portrait.png" />
    <splash density="port-xhdpi" src="res/screens/android/screen-xhdpi-portrait.png" />
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*" />
    <allow-intent href="itms-apps:*" />
  </platform>
  <vs:toolsetVersion xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">6.3.1</vs:toolsetVersion>
  <engine name="ios" spec="4.2.0" />
  <engine name="windows" spec="4.4.2" />
  <preference name="windows-target-version" value="10.0" />
  <preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="AIzaSyAYV4tKT88OZRJQ-z8EvF3xRTNWXY3pCBc" />
  <plugin name="com.red_folder.phonegap.plugin.backgroundservice" spec="~2.0.0" />
  <plugin name="com.red_folder.phonegap.plugin.backgroundservice.sample" spec="~2.0.0" />
  <plugin name="cordova-plugin-cleartext" spec="~1.0.0" />
  <plugin name="cordova-plugin-device" spec="~2.0.3" />
  <plugin name="cordova-plugin-dialogs" spec="~2.0.2" />
  <plugin name="cordova-plugin-file" spec="~6.0.2" />
  <plugin name="cordova-plugin-geolocation" spec="~4.0.2" />
  <plugin name="cordova-plugin-googlemaps" spec="~2.6.2" />
  <plugin name="cordova-plugin-media" spec="~5.0.3" />
  <plugin name="cordova-plugin-splashscreen" spec="~5.0.4-dev" />
  <plugin name="cordova-plugin-whitelist" spec="~1.3.4" />
  <preference name="Fullscreen" value="False" />
  <preference name="SplashScreenDelay" value="3000" />
  <preference name="ShowSplashScreenSpinner" value="false" />
  <preference name="KeepRunning" value="True" />
  <plugin name="cordova-plugin-background-mode" spec="~0.7.3" />
</widget>